  "scripts": {
    "watch": "tsc -w",
    "build": "tsc",
    "dev": "nodemon dist/index.js",
    "start": "node dist/index.js",
    "typeorm": "ts-node ./node_modules/typeorm/cli",
    "migration:run": "pnpm typeorm migration:run -- -d ./src/config/ormconfig.ts",
    "migration:generate": "pnpm typeorm migration:generate ./migrations/$pnpm_config_name -- -d ./src/config/ormconfigs.ts",
    "migration:create": "pnpm typeorm -- migration:create ./migrations/$npm_config_name",
    "migration:revert": "pnpm typeorm -- -d ./src/config/ormconfig.ts migration:revert"
  },
